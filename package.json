  "bugs": {
    "url": "https://github.com/Sayemahamed/all_auth/issues"
  },
  "homepage": "https://github.com/Sayemahamed/all_auth#readme",
    "devDependencies": {
    "concurrently": "^9.0.1",
    "tailwindcss": "^3.4.11"
  }
}
