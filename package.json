{
  "name": "all_auth",
  "version": "1.0.0",
  "description": "A starter template for django allauth using tailwind",
  "main": " ",
  "scripts": {
    "tailwind-dev": "npx tailwindcss -i ./static/input.css -o ./static/style.css --watch",
    "build": "npx tailwindcss -i ./static/input.css -o ./static/style.css --minify",
    "dev": "concurrently \"npm run tailwind-dev\" \"python manage.py runserver\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sayemahamed/all_auth.git"
  },
  "author": "MD Iftaker Ahamed Sayem",
